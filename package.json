{
  "name": "next-material-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-pro": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 next build --profile && rimraf export && next export  --threads=3 -o export",
    "build:clean": "rimraf .next && yarn build",
    "dev1": "next",
    "dev": "rimraf ./node_modules/.cache/babel-loader && rimraf .next && cross-env  NODE_OPTIONS='--inspect' next dev -p 3000",
    "export": "rimraf export && next export  --threads=3 -o export",
    "build": "next build",
    "start": "next start",
    "typescript": "tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "analyze": "cross-env ANALYZE=true yarn build"
  },
  "dependencies": {
    "@emotion/css": "^11.9.0",
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^2.8.8",
    "@mui/base": "^5.0.0-alpha.77",
    "@mui/icons-material": "^5.6.2",
    "@mui/lab": "^5.0.0-alpha.78",
    "@mui/material": "^5.6.2",
    "@mui/styles": "^5.6.2",
    "@mui/system": "^5.6.2",
    "@mui/types": "^7.1.3",
    "@types/nprogress": "^0.2.0",
    "@types/react-dom": "^18.0.2",
    "axios": "^0.26.1",
    "cross-env": "^7.0.3",
    "framer-motion": "^6.3.1",
    "jotai": "^1.6.4",
    "next": "12.1.5",
    "next-themes": "^0.1.1",
    "nprogress": "^0.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-query": "^4.0.0-beta.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.1.5",
    "@types/node": "17.0.26",
    "@types/react": "^18.0.6",
    "eslint": "8.14.0",
    "eslint-config-next": "12.1.5",
    "next-transpile-modules": "^9.0.0",
    "typescript": "4.6.3"
  }
}
