{
  "name": "next-material-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "export-pro": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 next build --profile && rimraf export && next export  --threads=3 -o export",
    "build:clean": "rimraf .next && pnpm build",
    "dev": "rimraf ./node_modules/.cache/babel-loader && rimraf .next && cross-env  NODE_OPTIONS='--inspect' next dev -p 3000",
    "export": "rimraf export && next export  --threads=3 -o export",
    "build": "next build",
    "start": "next start",
    "typescript": "tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "lint:fix": "eslint . --ext ts --ext tsx --ext js --fix",
    "analyze": "cross-env ANALYZE=true pnpm build"
  },
  "dependencies": {
    "@babel/core": "^7.19.3",
    "@emotion/cache": "^11.10.3",
    "@emotion/css": "^11.10.0",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.4",
    "@hookform/resolvers": "^2.9.8",
    "@mui/base": "5.0.0-alpha.100",
    "@mui/icons-material": "^5.10.6",
    "@mui/lab": "5.0.0-alpha.102",
    "@mui/material": "^5.10.8",
    "@mui/system": "^5.10.8",
    "@mui/types": "^7.2.0",
    "@tanstack/react-query": "^4.10.3",
    "@types/js-cookie": "^3.0.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash-es": "^4.17.6",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "axios": "^1.1.2",
    "clsx": "^1.2.1",
    "date-fns": "^2.29.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "framer-motion": "^7.5.3",
    "i18next": "^21.10.0",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lodash-es": "^4.17.21",
    "next": "12.3.1",
    "next-i18next": "^12.1.0",
    "next-themes": "^0.2.1",
    "notistack": "^2.0.5",
    "nprogress": "^0.2.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.37.0",
    "react-i18next": "^11.18.6",
    "recoil": "^0.7.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.3.1",
    "@types/node": "18.8.3",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.25.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "^12.3.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
