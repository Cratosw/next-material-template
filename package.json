{
  "name": "next-material-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "export-pro": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 next build --profile && rimraf export && next export  --threads=3 -o export",
    "build:clean": "rimraf .next && yarn build",
    "dev": "rimraf ./node_modules/.cache/babel-loader && rimraf .next && cross-env  NODE_OPTIONS='--inspect' next dev -p 3000",
    "export": "rimraf export && next export  --threads=3 -o export",
    "build": "next build",
    "start": "next start",
    "typescript": "tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "analyze": "cross-env ANALYZE=true yarn build"
  },
  "dependencies": {
    "@emotion/css": "^11.10.0",
    "@emotion/react": "^11.10.0",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@hookform/resolvers": "^2.8.10",
    "@mui/base": "^5.0.0-alpha.93",
    "@mui/icons-material": "^5.8.2",
    "@mui/lab": "^5.0.0-alpha.95",
    "@mui/material": "^5.10.1",
    "@mui/styles": "^5.8.0",
    "@mui/system": "^5.10.1",
    "@mui/types": "^7.1.5",
    "@tanstack/react-query": "^4.2.1",
    "@types/js-cookie": "^3.0.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash-es": "^4.17.6",
    "axios": "^0.27.2",
    "date-fns": "^2.29.1",
    "framer-motion": "^7.2.0",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lodash-es": "^4.17.21",
    "next": "12.2.5",
    "next-i18next": "^11.3.0",
    "next-themes": "^0.2.0",
    "notistack": "^2.0.5",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.34.2",
    "recoil": "^0.7.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.2.5",
    "@types/node": "18.7.6",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.5",
    "cross-env": "^7.0.3",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "next-transpile-modules": "^9.0.0",
    "typescript": "4.7.4"
  }
}
