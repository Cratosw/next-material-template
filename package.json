{
  "name": "next-material-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build-pro": "cross-env NODE_ENV=production NODE_OPTIONS=--max_old_space_size=4096 next build --profile && rimraf export && next export  --threads=3 -o export",
    "build:clean": "rimraf .next && yarn build",
    "dev1": "next",
    "dev": "rimraf ./node_modules/.cache/babel-loader && rimraf .next && cross-env  NODE_OPTIONS='--inspect' next dev -p 3000",
    "export": "rimraf export && next export  --threads=3 -o export",
    "build": "next build",
    "start": "next start",
    "typescript": "tsc -p tsconfig.json",
    "format": "prettier --write \"**/*.{js,ts,tsx}\"",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "analyze": "cross-env ANALYZE=true yarn build"
  },
  "dependencies": {
    "@emotion/css": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@hookform/resolvers": "^2.8.8",
    "@mui/base": "^5.0.0-alpha.68",
    "@mui/icons-material": "^5.4.1",
    "@mui/lab": "^5.0.0-alpha.68",
    "@mui/material": "^5.4.1",
    "@mui/styles": "^5.4.1",
    "@mui/system": "^5.4.1",
    "@mui/types": "^7.1.1",
    "@types/nprogress": "^0.2.0",
    "@types/react-dom": "^17.0.11",
    "axios": "^0.26.0",
    "cross-env": "^7.0.3",
    "framer-motion": "^6.2.6",
    "jotai": "^1.6.0",
    "next": "12.1.0",
    "next-themes": "^0.0.15",
    "nprogress": "^0.2.0",
    "react": "^18.0.0-rc.0",
    "react-dom": "^18.0.0-rc.0",
    "react-query": "^3.34.15",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.10",
    "@types/node": "17.0.18",
    "@types/react": "^17.0.39",
    "eslint": "8.9.0",
    "eslint-config-next": "12.1.0",
    "next-transpile-modules": "^9.0.0",
    "typescript": "4.5.5"
  }
}
